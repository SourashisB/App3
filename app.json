{
  "expo": {
    "name": "App3",
    "slug": "App3",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/LOGO.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "splash": {
      "backgroundColor": "#000000",
      "foregroundImage": "./assets/splash.jpg",
      "resizeMode": "contain"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.yourcompany.App3",
      "buildNumber": "1.0.0",
      "icon": "./assets/LOGO.png"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/LOGO.png",
        "backgroundColor": "#000000"
      },
      "package": "com.yourcompany.App3",
      "versionCode": 1
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "eas": {
        "projectId": "29f3a680-a89e-4290-9d39-60c40f3e23c6"
      }
    }
  }
}
